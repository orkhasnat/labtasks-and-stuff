# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  CurveDraw_config = debug

else ifeq ($(config),release)
  CurveDraw_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := CurveDraw

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CurveDraw:
ifneq (,$(CurveDraw_config))
	@echo "==== Building CurveDraw ($(CurveDraw_config)) ===="
	@${MAKE} --no-print-directory -C . -f CurveDraw.make config=$(CurveDraw_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CurveDraw.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   CurveDraw"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"